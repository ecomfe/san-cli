{
    "name": "san-cli-ui",
    "version": "1.0.3",
    "main": "commander.js",
    "license": "MIT",
    "engines": {
        "node": ">=8"
    },
    "scripts": {
        "start": "cross-env DEBUG=san-cli:ui:* SAN_CLI_UI_DEV=true SAN_VAR_APP_GRAPHQL_ENDPOINT=ws://localhost:8333/graphql nodemon ../san-cli/index.js ui",
        "serve": "cross-env SAN_VAR_APP_GRAPHQL_ENDPOINT=ws://localhost:8333/graphql node ../san-cli/index.js serve --config ./client/san.config.js",
        "ui": "yarn run build:all && yarn run prod",
        "build": "cross-env NODE_ENV=production node ../san-cli/index.js build --config ./client/san.config.js",
        "build:all": "yarn run build && cd ../san-cli-plugin-dashboard && yarn run build && cd ../san-cli-ui-widgets && yarn run build && cd ../san-cli-ui",
        "prepublishOnly": "yarn run build",
        "prod": "cross-env DEBUG=san-cli:ui:* node ../san-cli/index.js ui",
        "analyze": "cross-env NODE_ENV=production node ../san-cli/index.js build --config ./client/san.config.js --analyze",
        "start:noDebug": "cross-env SAN_CLI_UI_DEV=true SAN_VAR_APP_GRAPHQL_ENDPOINT=ws://localhost:8333/graphql nodemon ../san-cli/index.js ui",
        "debug:task": "cross-env DEBUG=san-cli:ui:tasks:connectors* SAN_CLI_UI_DEV=true SAN_VAR_APP_GRAPHQL_ENDPOINT=ws://localhost:8333/graphql nodemon ../san-cli/index.js ui",
        "debug:plugins": "cross-env DEBUG=san-cli:ui:plugins* SAN_CLI_UI_DEV=true SAN_VAR_APP_GRAPHQL_ENDPOINT=ws://localhost:8333/graphql nodemon ../san-cli/index.js ui",
        "debug:views": "cross-env DEBUG=san-cli:ui:views* SAN_CLI_UI_DEV=true SAN_VAR_APP_GRAPHQL_ENDPOINT=ws://localhost:8333/graphql nodemon ../san-cli/index.js ui",
        "dev:san": "node ../san-cli/index.js",
        "dev:san-ui": "cross-env DEBUG=san-cli:* SAN_CLI_UI_DEV=true node ../san-cli/index.js ui",
        "dev:san-init": "node ../san-cli/index.js init",
        "dev:san-remote-list": "node ../san-cli/index.js remote list",
        "test": "yarn run build:all && yarn run test:start",
        "test:start": "yarn run test:clear && start-server-and-test test:ui http://localhost:3338 test:e2e:start",
        "test:e2e:start": "cypress run",
        "test:clear": "rimraf ./__tests__/san-cli-ui-test",
        "test:debug": "start-server-and-test test:ui http://localhost:3338 test:e2e:start",
        "test:ui": "cross-env DEBUG=san-cli:ui:* SAN_CLI_UI_TEST=true node ../san-cli/index.js ui -p 3338"
    },
    "nodemonConfig": {
        "ignore": [
            "client/*",
            "public/*",
            "dist/*"
        ],
        "delay": "2000"
    },
    "repository": {
        "type": "git",
        "url": "git://github.com/ecomfe/san-cli"
    },
    "site": "https://ecomfe.github.io/san-cli",
    "keywords": [
        "san",
        "san.js",
        "webpack",
        "san-ui",
        "san-cli"
    ],
    "dependencies": {
        "@akryum/winattr": "^3.0.0",
        "@jinzhan/open-browser": "^0.0.1",
        "apollo-server-express": "^2.15.1",
        "chalk": "^4.0.0",
        "clone": "^2.1.2",
        "cosmiconfig": "^7.0.0",
        "execa": "^4.0.1",
        "express": "^4.17.1",
        "express-history-api-fallback": "^2.2.1",
        "fkill": "^7.0.1",
        "fs-extra": "^9.0.0",
        "globby": "^11.0.0",
        "graphql-type-json": "^0.3.1",
        "javascript-stringify": "^2.0.1",
        "js-yaml": "^3.14.0",
        "launch-editor": "^2.2.1",
        "lowdb": "^1.0.0",
        "node-fetch": "^2.6.0",
        "node-ipc": "^9.1.1",
        "node-notifier": "^7.0.0",
        "portfinder": "^1.0.25",
        "recast": "^0.19.1",
        "rss-parser": "^3.9.0",
        "san-cli-plugin-dashboard": "^1.0.0",
        "san-cli-ui-widgets": "^1.0.0",
        "san-cli-utils": "^1.0.0",
        "shortid": "^2.2.15",
        "util": "^0.12.3"
    },
    "devDependencies": {
        "@dicebear/avatars": "^4.2.1",
        "@dicebear/avatars-bottts-sprites": "^4.2.1",
        "@dicebear/avatars-gridy-sprites": "^4.2.1",
        "@dicebear/avatars-identicon-sprites": "^4.2.1",
        "@dicebear/avatars-initials-sprites": "^4.2.1",
        "@dicebear/avatars-jdenticon-sprites": "^4.2.1",
        "animate.css": "^4.1.0",
        "apollo-cache-inmemory": "^1.6.5",
        "apollo-client": "^2.6.8",
        "apollo-link-ws": "^1.0.20",
        "babel-plugin-import": "^1.13.0",
        "clean-webpack-plugin": "^3.0.0",
        "cross-env": "^7.0.2",
        "cypress": "^5.6.0",
        "deepmerge": "^4.2.2",
        "directory-named-webpack-plugin": "^4.0.1",
        "fastq": "^1.8.0",
        "graphql-tag": "^2.10.3",
        "load-script": "^1.0.0",
        "md5": "^2.3.0",
        "nodemon": "^2.0.4",
        "rimraf": "^3.0.2",
        "san": "~3.10.0",
        "san-component": "0.1.4",
        "san-router": "^1.2.2",
        "santd": "^0.3.1",
        "start-server-and-test": "^1.11.5",
        "subscriptions-transport-ws": "^0.9.16",
        "tiny-emitter": "^2.1.0",
        "xterm": "^4.7.0",
        "xterm-addon-fit": "^0.4.0",
        "xterm-addon-web-links": "^0.4.0"
    }
}
